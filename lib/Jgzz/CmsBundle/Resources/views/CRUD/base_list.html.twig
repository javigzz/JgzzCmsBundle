{% extends 'SonataAdminBundle:CRUD:base_list.html.twig' %}

{% block table_body %}
<tbody>
    {% for object in admin.datagrid.results %}
    <tr>
        {% for field_description in admin.list.elements %}
        {{ object|render_list_element(field_description) }}
        {% endfor %}

        <td>
        {# enlaces de edición según locale del contenido #}
        {% include 'JgzzCmsBundle:CRUD:botones_mlang_edit.html.twig' with { 'admin': admin } %}
        </td>
    </tr>
    {% endfor %} {# for objetos resultado #}
</tbody>
{% endblock %}

